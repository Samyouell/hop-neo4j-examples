    <modified_user>-</modified_user>
    <modified_date>2022/11/01 10:16:30.489</modified_date>
    <key_for_session_key>H4sIAAAAAAAAAAMAAAAAAAAAAAA=</key_for_session_key>
    <is_key_private>N</is_key_private>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>CSV file input - books and authors</from>
      <to>Neo4j Cypher - authors WROTE books</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>CSV file input - books and authors</name>
    <type>CSVInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <filename>${PROJECT_HOME}/files/books-and-authors.csv</filename>
    <filename_field/>
    <rownum_field/>
    <include_filename>N</include_filename>
    <separator>,</separator>
    <enclosure>"</enclosure>
    <header>Y</header>
    <buffer_size>50000</buffer_size>
    <lazy_conversion>Y</lazy_conversion>
    <add_filename_result>N</add_filename_result>
    <parallel>N</parallel>
    <newline_possible>N</newline_possible>
    <encoding>UTF-8</encoding>
    <fields>
      <field>
        <name>bookid</name>
        <type>Integer</type>
        <format>#</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>15</length>
        <precision>0</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>title</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>73</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>year</name>
        <type>Integer</type>
        <format>#</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>15</length>
        <precision>0</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>personid</name>
        <type>Integer</type>
        <format>#</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>15</length>
        <precision>0</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>name</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>18</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>born</name>
        <type>Integer</type>
        <format>#</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>15</length>
        <precision>0</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>publishedby</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>25</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>256</xloc>
      <yloc>336</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Neo4j Cypher - authors WROTE books</name>
    <type>Neo4jCypherOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>neo</connection>
    <cypher>UNWIND $rows AS row
MERGE ( a:Author {personId : row.personid} )
  ON CREATE SET
	a.name = row.name,
	a.born = row.born

MERGE ( b:Book {bookId : row.bookid} )
  ON CREATE SET
	b.title = row.title,
	b.published = row.year

MERGE (a) -[w:WROTE]- (b)
  ON CREATE SET
	w.publishedBy = row.publishedby</cypher>
    <batch_size>1000</batch_size>
    <read_only>N</read_only>
    <nr_retries_on_error/>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field/>
    <unwind>Y</unwind>
    <unwind_map>rows</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field/>
    <mappings>
      <mapping>
        <parameter>bookid</parameter>
        <field>bookid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>title</parameter>
        <field>title</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>year</parameter>
        <field>year</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>personid</parameter>
        <field>personid</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>name</parameter>
        <field>name</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>born</parameter>
        <field>born</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>publishedby</parameter>
        <field>publishedby</field>
        <type>String</type>
      </mapping>
    </mappings>
    <returns/>
    <attributes/>
    <GUI>
      <xloc>512</xloc>
      <yloc>336</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
