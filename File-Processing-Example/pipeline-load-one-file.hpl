      <from>csv-file-input</from>
      <to>Neo4j Cypher - create Humans nodes</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Neo4j Cypher - create Humans nodes</name>
    <type>Neo4jCypherOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>neo</connection>
    <cypher>UNWIND $rows AS row
MERGE ( h:Human {humanId : row.uniqueId} )
  ON CREATE SET
	h.birthDate = row.birthDate,
	h.externalId = row.externalId,
	h.firstName = row.firstName,	
	h.fullName = row.fullName,
	h.lastName = row.lastName,
	h.source = row.source
	</cypher>
    <batch_size>1000</batch_size>
    <read_only>N</read_only>
    <nr_retries_on_error>10</nr_retries_on_error>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field/>
    <unwind>Y</unwind>
    <unwind_map>rows</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field/>
    <mappings>
      <mapping>
        <parameter>lastName</parameter>
        <field>lastName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>uniqueId</parameter>
        <field>UniqueId</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>firstName</parameter>
        <field>firstName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>fullName</parameter>
        <field>fullName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>birthDate</parameter>
        <field>birthDate</field>
        <type>LocalDateTime</type>
      </mapping>
      <mapping>
        <parameter>externalId</parameter>
        <field>externalId</field>
        <type>Integer</type>
      </mapping>
      <mapping>
        <parameter>source</parameter>
        <field>source</field>
        <type>String</type>
      </mapping>
    </mappings>
    <returns/>
    <attributes/>
    <GUI>
      <xloc>448</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
