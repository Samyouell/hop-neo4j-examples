<?xml version="1.0" encoding="UTF-8"?>
<pipeline>
  <info>
    <name>pipeline--neo4jcypher-simple</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description/>
    <extended_description/>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2022/11/01 10:16:30.489</created_date>
    <modified_user>-</modified_user>
    <modified_date>2022/11/01 10:16:30.489</modified_date>
    <key_for_session_key>H4sIAAAAAAAAAAMAAAAAAAAAAAA=</key_for_session_key>
    <is_key_private>N</is_key_private>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>humans csv</from>
      <to>Neo4j Cypher - create Humans nodes</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Neo4j Cypher - create Humans nodes</name>
    <type>Neo4jCypherOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>neo</connection>
    <cypher>UNWIND $rows AS row
MERGE ( h:Human {humanId : row.uniqueId} )
  ON CREATE SET
	h.birthDate = row.birthDate,
	h.externalId = row.externalId,
	h.firstName = row.firstName,	
	h.fullName = row.fullName,
	h.lastName = row.lastName
	</cypher>
    <batch_size>1000</batch_size>
    <read_only>N</read_only>
    <nr_retries_on_error>10</nr_retries_on_error>
    <retry>Y</retry>
    <cypher_from_field>N</cypher_from_field>
    <cypher_field/>
    <unwind>Y</unwind>
    <unwind_map>rows</unwind_map>
    <returning_graph>N</returning_graph>
    <return_graph_field/>
    <mappings>
      <mapping>
        <parameter>lastName</parameter>
        <field>lastName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>uniqueId</parameter>
        <field>UniqueId</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>firstName</parameter>
        <field>firstName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>fullName</parameter>
        <field>fullName</field>
        <type>String</type>
      </mapping>
      <mapping>
        <parameter>birthDate</parameter>
        <field>birthDate</field>
        <type>LocalDateTime</type>
      </mapping>
      <mapping>
        <parameter>externalId</parameter>
        <field>externalId</field>
        <type>Integer</type>
      </mapping>
    </mappings>
    <returns/>
    <attributes/>
    <GUI>
      <xloc>384</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>humans csv</name>
    <type>CSVInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <filename>${PROJECT_HOME}/files/humans.csv</filename>
    <filename_field/>
    <rownum_field/>
    <include_filename>N</include_filename>
    <separator>,</separator>
    <enclosure>"</enclosure>
    <header>Y</header>
    <buffer_size>50000</buffer_size>
    <lazy_conversion>Y</lazy_conversion>
    <add_filename_result>N</add_filename_result>
    <parallel>N</parallel>
    <newline_possible>N</newline_possible>
    <encoding>UTF-8</encoding>
    <fields>
      <field>
        <name>lastName</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>11</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>UniqueId</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>11</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>firstName</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>10</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>fullName</name>
        <type>String</type>
        <format/>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>21</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>birthDate</name>
        <type>Date</type>
        <format>MM/dd/yyyy</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>-1</length>
        <precision>-1</precision>
        <trim_type>none</trim_type>
      </field>
      <field>
        <name>externalId</name>
        <type>Integer</type>
        <format>#</format>
        <currency>$</currency>
        <decimal>.</decimal>
        <group>,</group>
        <length>15</length>
        <precision>0</precision>
        <trim_type>none</trim_type>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>192</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
